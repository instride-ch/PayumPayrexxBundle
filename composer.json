{
  "name": "w-vision/payum-payrexx-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Payrexx Bundle",
  "keywords": [
    "coreshop",
    "payment",
    "payrexx",
    "ecommerce"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "w-vision AG",
      "email": "support@w-vision.ch",
      "homepage": "https://w-vision.ch",
      "role": "Web Development Agency"
    }
  ],
  "require": {
    "php": ">=7.4",
    "php-http/guzzle6-adapter": "^2.0",
    "w-vision/payum-payrexx": "1.0.x-dev"
  },
  "repositories": {
    "w-vision/payum-payrexx": {
      "type": "path",
      "url": "../payum-payrexx",
      "options": {
        "symlink": true
      }
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Wvision\\Payum\\PayrexxBundle\\": "src/Wvision/Payum/Payrexx"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    },
    "pimcore": {
      "bundles": [
        "Wvision\\Payum\\PayrexxBundle\\PayumPayrexxBundle"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
